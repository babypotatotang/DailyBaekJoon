    "N=int(input())\n",
    "A=list(map(int,input().split()))\n",
    "B=list(map(int,input().split()))\n",
    "\n",
    "sortedA=sorted(A)\n",
    "sortedB=sorted(B,reverse=True)\n",
    "\n",
    "result=0\n",
    "\n",
    "for i in range(N):\n",
    "    result+=sortedA[i]*sortedB[i]\n",
    "\n",
    "print(result)\n"
   ]
  }
