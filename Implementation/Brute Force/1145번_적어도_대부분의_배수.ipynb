   "source": [
    "#1145번: 적어도 대부분의 배수\n",
    "array=list(map(int,input().split()))\n",
    "result=1\n",
    "\n",
    "while 1:\n",
    "    chk=0\n",
    "    for num in array:\n",
    "        if result%num==0:\n",
    "            chk+=1\n",
    "\n",
    "    if chk>=3:\n",
    "        print(result)\n",
    "        break\n",
    "\n",
    "    result+=1"
   ]
  }
