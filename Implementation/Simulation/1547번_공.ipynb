    "def swap(a,b): # 위치 바꾸는 함수 \n",
    "    tmp = cups[a]\n",
    "    cups[a] = cups[b]\n",
    "    cups[b] = tmp\n",
    "\n",
    "n = int(input())\n",
    "cups = [-1,1,0,0] # 공이 있는 자리에 1\n",
    "for _ in range(n):\n",
    "    a, b = map(int,input().split())\n",
    "    swap(a,b)\n",
    "\n",
    "print(cups.index(1)) # 리스트 값이 1인 index 출력 "
   ]
  }
