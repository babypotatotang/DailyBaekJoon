    "N = int(input()) # 파일의 개수 \n",
    "Disk = list(map(int,input().split()))\n",
    "Size = int(input())\n",
    "num = 0 \n",
    "for D in Disk:\n",
    "    tmp = 0 \n",
    "    if D != 0: \n",
    "        tmp = D//Size+1 if D%Size else D//Size\n",
    "\n",
    "    num += tmp\n",
    "\n",
    "print(Size * num)\n"
   ]
  }
