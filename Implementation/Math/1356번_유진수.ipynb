    "def mul(nums):\n",
    "    result = 1\n",
    "    for num in nums:\n",
    "        result*=int(num)\n",
    "    return result\n",
    "\n",
    "\n",
    "num = input()\n",
    "ck=False\n",
    "\n",
    "for i in range(1,len(num)):\n",
    "    front = mul(num[:i])\n",
    "    end = mul(num[i:])\n",
    "    if front == end: ck = True; break\n",
    "\n",
    "print(\"YES\") if ck else print(\"NO\")"
   ]
  }
