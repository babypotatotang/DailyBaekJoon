   "source": [
    "# 11052번 카드구매하기\n",
    "\n",
    "N = int(input())\n",
    "Price = [0] + list(map(int,input().split()))\n",
    "\n",
    "Dp = [0] * (N+1)\n",
    "Dp[1] = Price[1]\n",
    "Dp[2] = max(Price[2], Price[1]*2)\n",
    "\n",
    "for i in range(3, N+1):\n",
    "    for j in range(1,i+1):\n",
    "        Dp[i] = max(Dp[i], Dp[i-j] + Price[j])\n",
    "\n",
    "print(Dp[N])"
   ]
  }
 ],
