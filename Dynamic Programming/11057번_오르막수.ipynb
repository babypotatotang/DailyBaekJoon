   "source": [
    "n = int(input())\n",
    "dp = [[0] * 10 for _ in range(10000)]\n",
    "\n",
    "for row in range(1,n+2):\n",
    "    for col in range(0,10):\n",
    "        if col == 0: \n",
    "            dp[row][col] = 1\n",
    "            continue\n",
    "        dp[row][col] = dp[row-1][col] + dp[row][col-1]\n",
    "        dp[row][col] %=10007\n",
    "\n",
    "print(dp[n+1][9])"
   ]
  }
 ],
