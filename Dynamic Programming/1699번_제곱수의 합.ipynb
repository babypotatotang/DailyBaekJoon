   "source": [
    "n = int(input())\n",
    "dp = [i for i in range(n+1)]\n",
    "\n",
    "for i in range(1,n+1):\n",
    "    for j in range(1,i):\n",
    "        if j*j > i: break\n",
    "        if dp[i] > dp[i-j*j] + 1:\n",
    "            dp[i] = dp[i-j*j] + 1\n",
    "\n",
    "print(dp[n])"
   ]
  }
