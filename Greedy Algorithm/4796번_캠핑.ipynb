    "cnt = 1\n",
    "while(1):\n",
    "    L,P,V = map(int,input().split())\n",
    "    if L == 0 and P == 0 and V == 0 : break\n",
    "    rest = V % P if V%P<L else L\n",
    "    result = (V//P) * L + rest\n",
    "    print(\"Case {}: {}\".format(cnt,result))\n",
    "    cnt+=1"
   ]
  }
