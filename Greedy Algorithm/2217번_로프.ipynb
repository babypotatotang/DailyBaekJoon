   "source": [
    "N = int(input())\n",
    "Ropes=[]; ResultWeight=0; \n",
    "for _ in range(N):\n",
    "    Ropes.append(int(input()))\n",
    "Ropes=sorted(Ropes,reverse=True)\n",
    "\n",
    "for num in range(1,N+1):\n",
    "    TmpWeight = num * Ropes[num-1]\n",
    "    ResultWeight = max(TmpWeight,ResultWeight)\n",
    "\n",
    "print(ResultWeight)\n"
   ]
  }
