      "source": [
        "a,b,c=map(int,input().split())\n",
        "\n",
        "def sum(a,b):\n",
        "    if b==1:\n",
        "        return a%c\n",
        "    else:\n",
        "        tmp= sum(a,b//2)\n",
        "        if b%2==0: \n",
        "            return (tmp*tmp)%c\n",
        "        else: \n",
        "            return (tmp*tmp*a)%c\n",
        "\n",
        "print(sum(a,b))"
      ]
    }
  ]
